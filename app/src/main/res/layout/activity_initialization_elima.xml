<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="0dp"
    android:layout_marginRight="0dp"
    android:paddingRight="0dp"
    android:paddingLeft="0dp"
    android:paddingBottom="0dp"
    android:paddingTop="0dp"
    android:background="@drawable/elima_dialog"
   >

    <ImageView
        android:layout_width="130dp"
        android:layout_height="40dp"
        app:srcCompat="@drawable/elima_cancel"
        android:id="@+id/imgElimaCancel"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:scaleType="centerCrop"
        android:layout_alignTop="@+id/imgSelectElima"
        android:layout_toRightOf="@+id/imgSelectElima"
        android:layout_toEndOf="@+id/imgSelectElima" />

    <ImageView
        app:srcCompat="@drawable/elima_select"
        android:id="@+id/imgSelectElima"
        android:layout_height="40dp"
        android:layout_width="130dp"
        android:scaleType="centerCrop"
        android:layout_marginBottom="39dp"
        android:layout_marginLeft="35dp"
        android:layout_marginStart="35dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

</RelativeLayout>