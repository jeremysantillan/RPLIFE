<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginLeft="0dp"
    android:layout_marginRight="0dp"
    android:paddingRight="0dp"
    android:paddingLeft="0dp"
    android:paddingBottom="0dp"
    android:paddingTop="0dp"
    android:background="#73000000">

    <ImageView
        android:layout_width="309dp"
        android:layout_height="442dp"
        app:srcCompat="@drawable/inventory_dialog"
        android:id="@+id/imageView6"
        android:scaleType="fitCenter"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp"

       />

    <ImageView
        android:layout_width="292dp"
        android:layout_height="260dp"
        app:srcCompat="@drawable/inventory_items"
        android:id="@+id/imgvInventoryItems"
        app:layout_constraintTop_toTopOf="@+id/imageView6"
        android:layout_marginStart="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="64dp"
        app:layout_constraintVertical_bias="0.69"
        app:layout_constraintBottom_toBottomOf="@+id/imageView6"
        android:layout_marginBottom="36dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp" />

    <com.example.dobit.rplife.Home.BebasTextView
    android:text="All"
        android:layout_height="wrap_content"
    android:id="@+id/weapons"
    android:textSize="18sp"
    android:textColor="#f9690e"
        app:layout_constraintBottom_toBottomOf="@+id/imgvInventoryItems"
    android:layout_marginBottom="16dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageView6"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_width="60dp"
        android:textAlignment="center" />

    <com.example.dobit.rplife.Home.BebasTextView
        android:text="Weapons"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:id="@+id/loot"
        android:textSize="18sp"
        android:textColor="#efefef"
        android:layout_marginStart="10dp"
        app:layout_constraintLeft_toRightOf="@+id/weapons"
        android:layout_marginLeft="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/imgvInventoryItems"
        android:layout_marginBottom="16dp"
        android:textAlignment="center" />


    <com.example.dobit.rplife.Home.BebasTextView
        android:text="Loot"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView5"
        android:textSize="18sp"
        android:textColor="#efefef"
        app:layout_constraintBottom_toBottomOf="@+id/imgvInventoryItems"
        android:layout_marginBottom="16dp"
        app:layout_constraintRight_toLeftOf="@+id/potions"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:textAlignment="center" />


    <com.example.dobit.rplife.Home.BebasTextView
        android:text="Potions"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:id="@+id/potions"
        android:textSize="18sp"
        android:textColor="#efefef"
        app:layout_constraintBottom_toBottomOf="@+id/imgvInventoryItems"
        android:layout_marginBottom="16dp"
        app:layout_constraintRight_toRightOf="@+id/imgvInventoryItems"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:textAlignment="center" />


</android.support.constraint.ConstraintLayout>