<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginLeft="0dp"
    android:layout_marginRight="0dp"
    android:paddingRight="0dp"
    android:paddingLeft="0dp"
    android:paddingBottom="0dp"
    android:paddingTop="0dp"
    android:background="@android:color/transparent">

    <ImageView
        android:layout_width="367dp"
        android:layout_height="524dp"
        app:srcCompat="@drawable/inventory_dialog"
        android:id="@+id/imageView6"
        android:scaleType="fitCenter"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp" />

    <ImageView
        android:layout_width="345dp"
        android:layout_height="290dp"
        app:srcCompat="@drawable/inventory_items"
        android:id="@+id/imgvInventoryItems"
        app:layout_constraintTop_toTopOf="@+id/imageView6"
        android:layout_marginStart="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="64dp"
        app:layout_constraintVertical_bias="0.69"
        app:layout_constraintBottom_toBottomOf="@+id/imageView6"
        android:layout_marginBottom="36dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp" />

    <com.example.dobit.rplife.Home.BebasEditText
        android:text="ALL"
        android:textColor="#F9690E"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvInventoryAll"
        app:layout_constraintBottom_toBottomOf="@+id/imgvInventoryItems"
        app:layout_constraintLeft_toLeftOf="@+id/imageView6"
        android:layout_marginStart="46dp"
        android:layout_marginLeft="46dp"
        android:layout_marginBottom="6dp"
        android:editable="false" />

    <com.example.dobit.rplife.Home.BebasEditText
        android:text="Weapons"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvInventoryWeapons"
        android:layout_marginStart="42dp"
        app:layout_constraintLeft_toRightOf="@+id/tvInventoryAll"
        android:layout_marginLeft="42dp"
        app:layout_constraintBottom_toBottomOf="@+id/imgvInventoryItems"
        android:layout_marginBottom="6dp"
        android:editable="false" />

    <com.example.dobit.rplife.Home.BebasEditText
        android:text="Loot"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvInventoryLoot"
        app:layout_constraintBottom_toBottomOf="@+id/imgvInventoryItems"
        android:layout_marginBottom="6dp"
        android:layout_marginStart="32dp"
        app:layout_constraintLeft_toRightOf="@+id/tvInventoryWeapons"
        android:layout_marginLeft="32dp"
        android:editable="false" />

    <com.example.dobit.rplife.Home.BebasEditText
        android:text="Potions"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvInventoryPotions"
        app:layout_constraintBottom_toBottomOf="@+id/imgvInventoryItems"
        android:layout_marginBottom="6dp"
        android:layout_marginStart="36dp"
        app:layout_constraintLeft_toRightOf="@+id/tvInventoryLoot"
        android:layout_marginLeft="36dp"
        android:editable="false" />


</android.support.constraint.ConstraintLayout>