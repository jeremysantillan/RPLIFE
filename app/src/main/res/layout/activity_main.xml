<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.dobit.rplife.FocusActivity">

    <ImageView
        android:layout_width="275dp"
        android:layout_height="504dp"
        app:srcCompat="@drawable/focus_bg"
        android:id="@+id/imageView12"
        tools:layout_editor_absoluteY="39dp"
        tools:layout_editor_absoluteX="25dp" />

    <com.example.dobit.rplife.Home.BebasEditText
        android:text="CODE ANDROID"
        android:textColor="#FFFF"
        android:background="#006442"
        android:layout_width="258dp"
        android:layout_height="43dp"
        android:id="@+id/textView2"
        android:textSize="36sp"
        android:textAlignment="center"
        tools:layout_editor_absoluteY="487dp"
        android:layout_marginEnd="24dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="24dp"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        app:layout_constraintHorizontal_bias="0.58" />

    <Button
        android:id="@+id/cancelTimer"
        android:layout_width="166dp"
        android:layout_height="44dp"
        android:text="START"
        app:layout_constraintRight_toRightOf="@+id/imageView12"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageView12"
        app:layout_constraintHorizontal_bias="0.63"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        android:layout_marginBottom="112dp" />

    <com.example.dobit.rplife.widget.CircleCountDownView
        android:id="@+id/circle_count_down_view"
        android:layout_width="1dp"
        android:layout_height="1dp"
        android:layout_gravity="center_horizontal"
        android:visibility="gone"
        android:layout_margin="5dp"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="0dp" />


    <Button
        android:id="@+id/startTimer"
        android:layout_width="166dp"
        android:layout_height="44dp"
        android:text="START"
        app:layout_constraintRight_toRightOf="@+id/imageView12"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageView12"
        app:layout_constraintHorizontal_bias="0.63"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        android:layout_marginBottom="112dp" />


</android.support.constraint.ConstraintLayout>
